FROM

/shared/Sanya/R50_Baseline/L2_Business/L21_SourceIntegration/DB2ENV/AS400/v_RecPrem_GI a
FULL JOIN
/shared/Sanya/R50_Baseline/L2_Business/L21_SourceIntegration/DB2ENV/AS400/v_RecPrem_T21 b 
ON (a.Polisnummer=b.Polisnummer and a.Boekjaar=b.Boekjaar)
