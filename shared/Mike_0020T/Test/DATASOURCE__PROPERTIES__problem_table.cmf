  <attribute name="SQL" type="STRING">--------------------------------------
-- SELECT /model/ALL_DATASOURCES &#x9;&#x9;        [published + shared: DATA_SOURCE]
--   INNER JOIN /system/ALL_USERS               [users]
--&#x9; OUTER JOIN /model/ALL_RESOURCE_ATTRIBUTES&#x9;[join with the above view to get datasource attributes]
--&#x9; OUTER JOIN /model/ALL_RESOURCE_ATTRIBUTES&#x9;[join with the above view to get resource attributes]
--
-- NOTE: This view does push-down.
--------------------------------------
---- ALL_DATASOURCES ----
--  Requires the following XML capabilities to be added to the adapter file for postgresql_9_1
--&#x9;&#x9;Location: TDV_HOME/conf/adapters/system/postgresql_9_1/postgresql_9_1_values.xml
--&#x9;&#x9;&lt;common:attribute xmlns:common=&quot;http://www.compositesw.com/services/system/util/common&quot;&gt;
--&#x9;&#x9;&#x9;&lt;common:name&gt;/custom/TO_TIMESTAMP(~number)&lt;/common:name&gt;
--&#x9;&#x9;&#x9;&lt;common:type&gt;STRING&lt;/common:type&gt;
--&#x9;&#x9;&#x9;&lt;common:value&gt;TO_TIMESTAMP($1)&lt;/common:value&gt;
--&#x9;&#x9;&#x9;&lt;common:configID&gt;TO_TIMESTAMP(~number)&lt;/common:configID&gt;
--&#x9;&#x9;&lt;/common:attribute&gt;
SELECT 
&#x9;  A.RESOURCE_ORIGIN
&#x9;, A.RESOURCE_ID
&#x9;, A.RESOURCE_NAME
&#x9;, A.PARENT_PATH
&#x9;, A.RESOURCE_PATH
&#x9;, A.RESOURCE_TYPE
&#x9;, A.RESOURCE_SUBTYPE
&#x9;, A.RESOURCE_LINK_TYPE
&#x9;, A.IS_PUBLISHED
&#x9;, A.GUID
&#x9;, A.DATASOURCE_TYPE
&#x9;, A.DATASERVICE_NAME
&#x9;, A.CATEGORY_NAME
&#x9;, A.CATALOG_NAME
&#x9;, A.SCHEMA_NAME
&#x9;, A.COLUMN_ID
&#x9;, A.COLUMN_NAME
&#x9;, A.COLUMN_TYPE
&#x9;, A.COLUMN_BASE
&#x9;, A.COLUMN_SIZE
&#x9;, A.COLUMN_SCALE
&#x9;, A.COLUMN_ORDER
&#x9;, A.COLUMN_DIRECTION
&#x9;, A.ANNOTATION
&#x9;, A.OWNER_ID
&#x9;, A.OWNER_NAME
&#x9;, A.OWNER_DOMAIN
&#x9;, A.CREATOR_USER_ID
&#x9;, A.CREATOR_USER_NAME
&#x9;, A.CREATOR_USER_DOMAIN
&#x9;, A.CREATOR_DATE
&#x9;, A.CREATOR_DATE_INT
&#x9;, A.LAST_MODIFIED_USER_ID
&#x9;, A.LAST_MODIFIED_USER_NAME
&#x9;, A.LAST_MODIFIED_USER_DOMAIN
&#x9;, A.LAST_MODIFIED_DATE
&#x9;, A.LAST_MODIFIED_DATE_INT
--&#x9;, D.LAST_MODIFIED_DATE_INT
&#x9;, A.DATASOURCE_ADAPTER_NAME
&#x9;, D.DATASOURCE_ADAPTER_TYPE
&#x9;, D.DATASOURCE_HOSTNAME
&#x9;, D.DATASOURCE_PORT
&#x9;, D.DATASOURCE_DATABASE_NAME
&#x9;, D.DATASOURCE_LOGIN
&#x9;, D.DATASOURCE_PASSWORD
&#x9;, D.DATASOURCE_URL
&#x9;, D.DATASOURCE_PATTERN_STR
&#x9;, D.DATASOURCE_ROOT
&#x9;, D.DATASOURCE_CACHE_STATUS
&#x9;, D.DATASOURCE_CACHE_TRACKING
FROM (
---- ALL_DATASOURCES ----
SELECT {OPTION IGNORE_TRAILING_SPACES=&quot;FALSE&quot;, CASE_SENSITIVE=&quot;TRUE&quot;} 
&#x9;  CAST(&apos;ALL_DATASOURCES&apos; AS VARCHAR(30)) RESOURCE_ORIGIN
&#x9;, P.DATASOURCE_ID RESOURCE_ID
&#x9;, CAST(CASE WHEN STRPOS(P.DATASOURCE_NAME, &apos;/&apos;) &gt; 0 THEN &apos;&quot;&apos;||P.DATASOURCE_NAME||&apos;&quot;&apos;
&#x9;&#x9;&#x9;  ELSE P.DATASOURCE_NAME
&#x9;&#x9; END AS VARCHAR(255)) RESOURCE_NAME
&#x9;, CAST(P.PARENT_PATH AS VARCHAR(4000)) PARENT_PATH
&#x9;, REPLACE(CAST(P.PARENT_PATH || &apos;/&apos; || 
&#x9;&#x9;CASE WHEN STRPOS(P.DATASOURCE_NAME, &apos;/&apos;) &gt; 0 THEN &apos;&quot;&apos;||P.DATASOURCE_NAME||&apos;&quot;&apos;
&#x9;&#x9;&#x9;  ELSE P.DATASOURCE_NAME
&#x9;&#x9; END AS VARCHAR(4000)), &apos;//&apos;, &apos;/&apos;) RESOURCE_PATH
&#x9;, CAST(&apos;DATA_SOURCE&apos; AS VARCHAR(40)) RESOURCE_TYPE
&#x9;, CAST(P.ADAPTER_TYPE_CATEGORY AS VARCHAR(40)) RESOURCE_SUBTYPE
&#x9;, CAST(NULL AS VARCHAR(40)) RESOURCE_LINK_TYPE
&#x9;, CAST(CASE
&#x9;&#x9;WHEN P.IS_PUBLISHED IS NOT NULL  THEN SUBSTRING(P.IS_PUBLISHED,1,1)
&#x9;&#x9;ELSE P.IS_PUBLISHED
&#x9;  END AS VARCHAR(1)) IS_PUBLISHED
&#x9;, CAST(P.GUID AS VARCHAR(255)) GUID
&#x9;, CAST(CASE
&#x9;&#x9;WHEN STRPOS(P.PARENT_PATH, &apos;/services/databases&apos;) = 1  THEN &apos;VirtualRelational&apos;
&#x9;&#x9;WHEN STRPOS(P.PARENT_PATH, &apos;/services/webservices&apos;) = 1 THEN &apos;VirtualWsdl&apos;
&#x9;&#x9;ELSE NULL
&#x9;  END AS VARCHAR(20)) DATASOURCE_TYPE
&#x9;, P.DATASOURCE_NAME DATASERVICE_NAME
&#x9;, CAST(NULL AS VARCHAR(255)) CATEGORY_NAME
&#x9;, CAST(NULL as VARCHAR(255)) CATALOG_NAME
&#x9;, CAST(NULL as VARCHAR(255)) SCHEMA_NAME
&#x9;, CAST(NULL AS INTEGER) COLUMN_ID
&#x9;, CAST(NULL AS VARCHAR(255)) COLUMN_NAME
&#x9;, CAST(NULL AS VARCHAR(50)) COLUMN_TYPE
&#x9;, CAST(NULL AS VARCHAR(50)) COLUMN_BASE
&#x9;, CAST(NULL AS DECIMAL(19,0)) COLUMN_SIZE
&#x9;, CAST(NULL AS DECIMAL(19,0)) COLUMN_SCALE
&#x9;, CAST(NULL AS INTEGER) COLUMN_ORDER
&#x9;, CAST(NULL AS VARCHAR(10)) COLUMN_DIRECTION
&#x9;, P.ANNOTATION
&#x9;, P.ADAPTER_NAME DATASOURCE_ADAPTER_NAME
&#x9;, CAST(NULL AS VARCHAR(50)) DATASOURCE_ADAPTER_TYPE
&#x9;, CAST(NULL AS VARCHAR(255)) DATASOURCE_HOSTNAME
&#x9;, CAST(NULL AS INTEGER) DATASOURCE_PORT
&#x9;, CAST(NULL AS VARCHAR(100)) DATASOURCE_DATABASE_NAME
&#x9;, CAST(NULL AS VARCHAR(255)) DATASOURCE_LOGIN
&#x9;, CAST(NULL AS VARCHAR(255)) DATASOURCE_PASSWORD
&#x9;, CAST(NULL AS VARCHAR(500)) DATASOURCE_URL
&#x9;, CAST(NULL AS VARCHAR(500)) DATASOURCE_PATTERN_STR
&#x9;, CAST(NULL AS VARCHAR(500)) DATASOURCE_ROOT
&#x9;, CAST(NULL AS VARCHAR(4000)) DATASOURCE_CACHE_STATUS
&#x9;, CAST(NULL AS VARCHAR(4000)) DATASOURCE_CACHE_TRACKING
&#x9;-- User Information - OWNER
&#x9;, P.OWNER_ID
&#x9;, CAST(P.OWNER AS VARCHAR(60)) OWNER_NAME
&#x9;, CAST(AU.DOMAIN_NAME AS VARCHAR(255)) OWNER_DOMAIN
&#x9;-- User Information - CREATOR
&#x9;, P.DATASOURCE_CREATOR_ID CREATOR_USER_ID
&#x9;, AC.USERNAME CREATOR_USER_NAME
&#x9;, AC.DOMAIN_NAME CREATOR_USER_DOMAIN
&#x9;, CAST(
&#x9;&#x9;CASE WHEN P.DATASOURCE_CREATION_TIMESTAMP IS NULL OR P.DATASOURCE_CREATION_TIMESTAMP=0 THEN NULL
&#x9;&#x9;&#x9; ELSE TO_TIMESTAMP(TRUNC(CAST(P.DATASOURCE_CREATION_TIMESTAMP AS BIGINT)/1000)) 
&#x9;&#x9;END AS TIMESTAMP
&#x9;  ) AS CREATOR_DATE 
&#x9;, P.DATASOURCE_CREATION_TIMESTAMP CREATOR_DATE_INT 
&#x9;-- User Information - MODIFIER
&#x9;, P.DATASOURCE_MODIFIER_ID LAST_MODIFIED_USER_ID 
&#x9;, AM.USERNAME LAST_MODIFIED_USER_NAME
&#x9;, AM.DOMAIN_NAME LAST_MODIFIED_USER_DOMAIN
&#x9;, CAST(
&#x9;&#x9;CASE WHEN P.DATASOURCE_MODIFICATION_TIMESTAMP IS NULL OR P.DATASOURCE_MODIFICATION_TIMESTAMP=0 THEN NULL
&#x9;&#x9;&#x9; ELSE TO_TIMESTAMP(TRUNC(CAST(P.DATASOURCE_MODIFICATION_TIMESTAMP AS BIGINT)/1000)) 
&#x9;&#x9;END AS TIMESTAMP
&#x9;  ) AS LAST_MODIFIED_DATE 
&#x9;, CAST(P.DATASOURCE_MODIFICATION_TIMESTAMP AS BIGINT) AS LAST_MODIFIED_DATE_INT 
FROM
&#x9;/services/databases/system/model/ALL_DATASOURCES P
INNER JOIN /services/databases/system/ALL_USERS AU
&#x9;ON P.OWNER_ID = AU.USER_ID
  LEFT OUTER JOIN /services/databases/system/ALL_USERS AC
&#x9;ON P.DATASOURCE_CREATOR_ID = AC.USER_ID
  LEFT OUTER JOIN /services/databases/system/ALL_USERS AM
&#x9;ON P.DATASOURCE_MODIFIER_ID = AM.USER_ID
) A
&#x9;-- Join model/ALL_RESOURCE_ATTRIBUTES [datasource attributes]
&#x9;-- Get the data source attributes from model/ALL_RESOURCE_ATTRIBUTES
&#x9;LEFT OUTER JOIN (
&#x9;&#x9;SELECT 
&#x9;&#x9;  DATASOURCE_ID
--&#x9;&#x9;, CAST(p0.PROPERTY_VALUE AS BIGINT) LAST_MODIFIED_DATE_INT
&#x9;&#x9;-- Data source attributes
&#x9;&#x9;, CAST(CASE WHEN p9.PROPERTY_VALUE IS NULL OR LENGTH(p9.PROPERTY_VALUE)=0 THEN NULL
&#x9;&#x9;&#x9;&#x9;ELSE p9.PROPERTY_VALUE
&#x9;&#x9;&#x9;END AS VARCHAR(255)) AS DATASOURCE_ADAPTER_TYPE
